<div class="ced-outer-box">
  <div class="ced-svg">
  <svg xmlns="http://www.w3.org/2000/svg" viewBox="0 0 1000 100" preserveAspectRatio="none">
      <path class="elementor-shape-fill" opacity="0.33" d="M473,67.3c-203.9,88.3-263.1-34-320.3,0C66,119.1,0,59.7,0,59.7V0h1000v59.7 c0,0-62.1,26.1-94.9,29.3c-32.8,3.3-62.8-12.3-75.8-22.1C806,49.6,745.3,8.7,694.9,4.7S492.4,59,473,67.3z"></path>
      <path class="elementor-shape-fill" opacity="0.66" d="M734,67.3c-45.5,0-77.2-23.2-129.1-39.1c-28.6-8.7-150.3-10.1-254,39.1 s-91.7-34.4-149.2,0C115.7,118.3,0,39.8,0,39.8V0h1000v36.5c0,0-28.2-18.5-92.1-18.5C810.2,18.1,775.7,67.3,734,67.3z"></path>
      <path class="elementor-shape-fill" d="M766.1,28.9c-200-57.5-266,65.5-395.1,19.5C242,1.8,242,5.4,184.8,20.6C128,35.8,132.3,44.9,89.9,52.5C28.6,63.7,0,0,0,0 h1000c0,0-9.9,40.9-83.6,48.1S829.6,47,766.1,28.9z"></path>
  </svg>	
  </div>

  <div class="container">
    <header class="section__header ced-illustration-heading-outer">
      <div class="section__header-stack">
        <h2 class="section__title heading h3">{{ section.settings.title}}</h2>
      </div>
      {{ section.settings.content}}
      {%- if section.settings.link_title != blank -%}
        <a href="{{ section.settings.link_url }}" class="section__action-link link">{{ section.settings.link_title }} {% render 'paw-icon' %}</a>
      {%- endif -%}
    </header>
  </div> 
</div>

<div class="ced-illustration-image">
<img src="https://cdn.shopify.com/s/files/1/0636/8208/5081/files/club3.webp?v=1651055804">
</div>


{% schema %}
{
  "name": "Custom Illustration",
  "settings": [
	{
      "type": "text",
      "id": "title",
      "label": "Heading",
      "default": "Heading"
    },
	{
      "type": "richtext",
      "id": "content",
      "label": "Content"
    },
    {
      "type": "text",
      "id": "link_title",
      "label": "Link title",
      "default": "View all"
    },
    {
      "type": "url",
      "id": "link_url",
      "label": "Link URL"
    }
	
  ],

	"presets": [
    {
      "category": "text",
      "name": "Custom Illustration ",
      "settings": {}
    }
  ]
}

{% endschema %}


<style>
  
  .ced-outer-box{background: var(--secondary-button-background);
    height:500px;}

    .ced-svg svg{
    width: calc(100% + 1.3px);
      height: 140px;
    }
    .ced-svg svg path{
    fill: #fff;
    }
  
  .ced-illustration-heading-outer{
  	display: flex;
    flex-direction: column;
    justify-content: center;
    align-items: center;
    padding: 30px;
    margin-bottom:100px;
    max-width: 600px;
    text-align: -webkit-center;
    text-align: center;
    margin: auto;}

  	.ced-illustration-image{
    margin-top: -140px;
    max-width: 700px;
    width: 100%;
    margin-left: auto;
    margin-right: auto;
    
    }
  
  .ced-outer-box .section__header .section__title{font-size:3rem;
  margin-bottom:20px;}
  
  .ced-outer-box .section__header .section__title , .ced-outer-box .section__header p , .ced-outer-box .section__header .section__action-link , .ced-outer-box .section__header .section__action-link .ced-paw-icon{color:#fff;
  fill:#fff;}

  .ced-outer-box .section__header .section__action-link{margin-left:-20px;}

</style>